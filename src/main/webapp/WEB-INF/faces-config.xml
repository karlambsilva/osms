<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    		  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    		  version="2.2">
    		  
    <application>
    	<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>    	
	</application>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateCategory</from-outcome>
			<to-view-id>/category/updateCategory.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>    
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteCategory</from-outcome>
			<to-view-id>/category/deleteCategory.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>  
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchCategory</from-outcome>
			<to-view-id>/category/searchCategory.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>  
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateProduct</from-outcome>
			<to-view-id>/product/updateProduct.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>    
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteProduct</from-outcome>
			<to-view-id>/product/deleteProduct.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>  
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchProduct</from-outcome>
			<to-view-id>/product/searchProduct.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>  
		
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateOrder</from-outcome>
			<to-view-id>/order/updateOrder.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>    
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteOrder</from-outcome>
			<to-view-id>/order/deleteOrder.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>  
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchOrder</from-outcome>
			<to-view-id>/order/searchOrder.xhtml</to-view-id>
		</navigation-case>	
	</navigation-rule>	 
</faces-config>
